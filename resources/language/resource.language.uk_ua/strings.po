msgctxt "#32000"
msgid "RSS Editor"
msgstr "RSS Редактор"

msgctxt "#32001"
msgid "Change Set"
msgstr "Змінити набір"

msgctxt "#32002"
msgid "items"
msgstr "пункти"

msgctxt "#32003"
msgid "Pages"
msgstr "Сторінки"

msgctxt "#32006"
msgid "Edit Attributes"
msgstr "Редагувати"

msgctxt "#32012"
msgid "RSS Feed URL"
msgstr "URL RSS каналу"

msgctxt "#32013"
msgid "Update Interval"
msgstr "Частота оновлення"

msgctxt "#32014"
msgid "Enter the paths of your RSS feeds %s"
msgstr "Введіть шляху ваших RSS-каналів %s"

msgctxt "#32024"
msgid "Select a Set "
msgstr "Вибрати набір"

msgctxt "#32025"
msgid "Set ID"
msgstr "Встановити ID"

msgctxt "#32027"
msgid "Enable Right to Left Text for this Set?"
msgstr "Включити текст справа наліво для цього набору?"

msgctxt "#32030"
msgid "Set Editor"
msgstr "Редактор набору"

msgctxt "#32040"
msgid "Error: "
msgstr "Помилка: "

msgctxt "#32041"
msgid "was either not found in the system,"
msgstr "був або не знайдений в системі,"

msgctxt "#32042"
msgid "or does not contain parsable XML."
msgstr "або не містить правильний XML."

msgctxt "#32043"
msgid "Try deleting it from your userdata folder and restarting KODI."
msgstr "Спробуйте видалити його з папки користувача та перезапустити KODI."

msgctxt "#32045"
msgid "Cannot Delete Set 1"
msgstr "Неможливо видалити Набір 1"

msgctxt "#32046"
msgid "Set 1 is required.  You cannot delete this."
msgstr "Набір 1 потрібен. Ви не можете його видалити."

msgctxt "#32047"
msgid "Reset set 1 to default?"
msgstr "Скинути набір 1 до значень за замовчуванням?"

msgctxt "#32050"
msgid "Set %s already exists."
msgstr "Набір %s вже існує."

msgctxt "#32051"
msgid "Your RSSFeeds.xml file is corrupt."
msgstr "Ваш RSSFeeds.xml файл пошкоджений."

msgctxt "#32052"
msgid "Attempt to regenerate"
msgstr "Спроба відновити"

msgctxt "#32053"
msgid "You will loose all previous settings"
msgstr "Ви втратите всі попередні налаштування"
