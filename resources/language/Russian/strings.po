msgctxt "#32000"
msgid "RSS Editor"
msgstr "RSS Редактор"

msgctxt "#32030"
msgid "Set Editor"
msgstr "Редактор наборов"

msgctxt "#32001"
msgid "Change Set"
msgstr "Изменить набор"

msgctxt "#32004"
msgid "Feed"
msgstr "Канал"

msgctxt "#32005"
msgid "Set"
msgstr "Набор"

msgctxt "#32006"
msgid "Edit Attributes"
msgstr "Изм. атрибуты"

msgctxt "#32011"
msgid "Select a feed to remove"
msgstr "Выберите канал для удаления"

msgctxt "#32012"
msgid "RSS Feed URL"
msgstr "RSS канал URL"

msgctxt "#32013"
msgid "Update Interval"
msgstr "Интервал обновления"

msgctxt "#32014"
msgid "Enter the paths of your RSS feeds %s"
msgstr "Введите пути ваших RSS-каналов %s"

msgctxt "#32020"
msgid "What Are Sets?"
msgstr "Какие наборы?"

msgctxt "#32021"
msgid "To use different sets in your skin,"
msgstr "Для того, чтобы использовать различные наборы в вашем скине,"

msgctxt "#32022"
msgid "each must be called from the skin"
msgstr "каждый из них должн быть вызван из скина"

msgctxt "#32023"
msgid "with a unique id"
msgstr "с уникальным ID"

msgctxt "#32024"
msgid "Select a Set "
msgstr "Выбрать набор "

msgctxt "#32025"
msgid "Set ID"
msgstr "Установить ID"

msgctxt "#32026"
msgid "Set %s already exists"
msgstr "Набор %s существует"

msgctxt "#32027"
msgid "Enable Right to Left Text for this Set?"
msgstr "Включить Справа Налево текст для этого набора?"

msgctxt "#32040"
msgid "Error: "
msgstr "Ошибка: "

msgctxt "#32041"
msgid "was either not found in the system,"
msgstr "не найден в системе,"

msgctxt "#32042"
msgid "or does not contain parsable XML."
msgstr "или содержит неправильный XML."

msgctxt "#32043"
msgid "Try deleting it from your userdata folder and restarting KODI."
msgstr "Попробуйте удалить его из папки пользовательских данных и перезапустить KODI."

msgctxt "#32044"
msgid "To Remove"
msgstr "Удалить"

msgctxt "#32045"
msgid "Cannot Delete Set 1"
msgstr "Не удается удалить набор 1"

msgctxt "#32046"
msgid "Set 1 is required.  You cannot delete this."
msgstr "Набор 1 необходим. Вы не можете удалять его."

msgctxt "#32047"
msgid "Reset set 1 to default?"
msgstr "Сбросить набор 1 на значения по умолчанию?"

msgctxt "#32048"
msgid "Reset Required"
msgstr "Сброс обязателен"

msgctxt "#32049"
msgid "You need to restart KODI for your changes to take effect"
msgstr "Вам необходимо перезапустить KODI для того чтобы ваши изменения вступили в силу"

msgctxt "#32050"
msgid "Set %s already exists."
msgstr "Набор %s существует"

msgctxt "#32051"
msgid "Your RSSFeeds.xml file is corrupt."
msgstr "Файл RSS Feeds.xml поврежден."

msgctxt "#32052"
msgid "Attempt to regenerate"
msgstr "Попытка восстановить"

msgctxt "#32053"
msgid "You will loose all previous settings"
msgstr "Вы потеряете все предыдущие настройки"